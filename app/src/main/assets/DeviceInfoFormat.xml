<?xml version="1.0" encoding="UTF-8"?>
<AppConfig>
    <Identify>%s</Identify> <!-- 002线路ID， 007站点双程号， 01屏号 -->
    <RouteLine>%s</RouteLine> <!-- 公交线路ID -->
    <StationID>%s</StationID> <!-- 当前公交站点ID -->
    <ServerIp>%s</ServerIp>
    <ServerPort2>%s</ServerPort2>
    <ServerPort>%s</ServerPort>
    <InfoPublishServer>%s</InfoPublishServer>
    <DevType>%x</DevType> <!-- 11:videoPost 12:signPost 13:linePost 十六进制 -->
    <CurrentStationID>%d</CurrentStationID>
    <NextStationID>%d</NextStationID>
    <ThemeStyle>%d</ThemeStyle> <!-- 1默认， 2镜像， 3第二主题， 4第二主题镜像 -->
    <TextType>%d</TextType> <!-- default: 0 -->
    <TextColor>#%08x</TextColor> <!-- ARGB #FFFFFFFF 为完全不透明的白色 -->
    <TextSize>%d</TextSize> <!-- 0: small  1: middle  2: large -->
    <TextFont>%s</TextFont> <!-- heiti: 黑体 songti: 宋体 kaiti: 楷体 lishu: 隶书 -->
    <TextSpeed>%d</TextSpeed> <!-- 0:slow 1:normal 2:fast -->
    <PicDispTime>%d</PicDispTime> <!-- ms -->
    <VideoPlayTime>%d</VideoPlayTime> <!-- second, default invalid 0 -->
</AppConfig>
